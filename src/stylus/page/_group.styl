.group__main
  overflow-x hidden
  position relative
  text-align center
  z-index: 0

.topIcon
  width 20%

.groupContents
  padding 5vh 3vw
  &__number
    background-color c-txt-1
    color c-white
    font-size 2rem
    margin 1rem
    width 6rem
  &__groupName
    font-size 2.3rem
    font-weight 500
    letter-spacing 0.2rem
    margin 2rem 1rem
    text-align left
    width 18rem
  & img.groupContents__groupIcon
    width 7rem
